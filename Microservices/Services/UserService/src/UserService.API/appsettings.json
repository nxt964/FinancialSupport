{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "__CONNECTION_STRING__"
  },
  "JwtSettings": {
    "Key": "__JWT_SECRET__",
    "Issuer": "UserService",
    "RefreshTokenLifespanMinutes": 1440,
    "AccessTokenLifespanMinutes": 15
  },
  "AllowedHosts": "*",
  "Redis": {
    "ConnectionString": "__REDIS_CONNECTION_STRING__",
    "InstanceName": "UserService:"
  },
  "Kafka": {
    "BootstrapServers": "__KAFKA_BOOTSTRAP_SERVER__"
  }
}
